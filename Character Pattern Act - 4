#include <iostream>
using namespace std;
int main()
{
    int test;
    cin>>test;
    while(test--){
        int l,c,h,w;
    cin>>l>>c>>h>>w;
    if(l==0 or c==0)
    {continue;}
    for(int i=0;i<(l*h)+l+1;i++){
        for(int j=0;j<(c*w)+c+1;j++){
           if(i%(h+1)==0)
           cout<<"*";
           else if(j%(w+1)==0)
           cout<<"*";
           else
           cout<<".";
        }
        cout<<'\n';
    }cout<<"\n";
 
    }
    return 0;
} 

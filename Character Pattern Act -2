#include <iostream>
using namespace std;
 
int main()
{
    int test;
    cin>>test;
    while(test--){
        int l,c;
        cin>>l>>c;
        for(int i=0;i<l;i++){
            for(int j=0;j<c;j++){
                if(i==0 or i==l-1)
                cout<<"*";
                else if(j==0 or j==c-1)
                cout<<"*";
                else
                cout<<".";
            }cout<<"\n";
        }
        cout<<"\n";
    }
 
    return 0;
}
